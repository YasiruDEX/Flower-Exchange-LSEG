// CSV Order Processing
digraph {
	A [label=Start]
	B [label="Read CSV File"]
	C [label="Validate Order"]
	D [label="Invalid Order: Write to CSV (Reject)"]
	E [label="Valid Order: Add to Buy/Sell Table"]
	F [label="Sort Buy Table (Descending by Price)"]
	G [label="Sort Sell Table (Ascending by Price)"]
	H [label="Attempt to Match Orders"]
	I [label="Order Matched: Mark Fill or PFill"]
	J [label="No Match: Keep in Table"]
	K [label="Write Execution Time to CSV"]
	L [label=End]
	A -> B [label="Start Process"]
	B -> C [label="Read Order"]
	C -> D [label="Invalid Order"]
	C -> E [label="Valid Order"]
	E -> F [label="Add to Buy/Sell Table"]
	F -> G [label="Sort Buy Table"]
	G -> H [label="Sort Sell Table"]
	H -> I [label="Match Found"]
	H -> J [label="No Match Found"]
	I -> K [label="Write Matched Order to CSV"]
	J -> K [label="Write Unmatched Order to CSV"]
	K -> L [label="Finish Process"]
}
